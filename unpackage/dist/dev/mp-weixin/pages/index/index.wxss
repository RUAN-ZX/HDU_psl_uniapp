.background {
  z-index: 99;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100vh;
  position: fixed;
  width: 100vw;
}
.background .preface {
  width: 100%;
}
.ellipsis {
  word-break: break-all;
  white-space: nowrap;
  text-overflow: ellipsis;
  /* 超出部分省略号 */
  overflow: hidden;
  /*超出隐藏*/
}
.card {
  padding: 20rpx;
  background-color: #ffffff;
  box-shadow: 6rpx 6rpx 15rpx 3rpx rgba(0, 0, 0, 0.21);
  border-radius: 15rpx;
  z-index: 1;
  position: relative;
}

